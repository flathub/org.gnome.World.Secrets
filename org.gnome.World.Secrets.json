    "modules" : [
        "shared-modules/libusb/libusb.json",
        "python3-pykeepass.json",
        "python3-pyotp.json",
        "python3-validators.json",
        "python3-zxcvbn-rs-py.json",
        "python3-python-yubico.json",
        "python3-pykcs11.json",
        {
            "name" : "secrets",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/World/secrets/-/archive/10.0/secrets-10.0.tar.gz",
                    "sha256": "ea0177d7a3a31f39ca7ae358578981cfe5680ea4db1ddd40e62f434c9ca4811e"
                }
            ]
        }
    ]
