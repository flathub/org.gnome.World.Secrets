{
    "id": "org.gnome.World.Secrets",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "47",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "secrets",
    "finish-args" : [
        "--device=all",
        "--device=dri",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=pcsc",
        "--socket=wayland"
    ],
    "cleanup" : [
        "*.a",
        "*.la",
        "/include",
        "/lib/cmake",
        "/lib/pkgconfig",
        "/man",
        "/share/aclocal",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig"
